<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="Toplevel_1">
    <property name="background">#2e2e2e</property>
    <property name="cursor">arrow</property>
    <property name="height">250</property>
    <property name="iconbitmap">ABP-blanco-en-fondo-negro.ico</property>
    <property name="minsize">280|440</property>
    <property name="overrideredirect">False</property>
    <property name="resizable">none</property>
    <property name="title" translatable="yes">Convertidor de CSV</property>
    <property name="width">325</property>
    <child>
      <object class="ttk.Label" id="Label_3">
        <property name="background">#2e2e2e</property>
        <property name="image">ABP blanco en fondo negro111.png</property>
        <layout manager="pack">
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_1">
        <property name="background">#2e2e2e</property>
        <property name="compound">top</property>
        <property name="foreground">#ffffff</property>
        <property name="justify">right</property>
        <property name="relief">flat</property>
        <property name="takefocus">false</property>
        <property name="text" translatable="yes">Convertidor masivo de CSVs a Excels</property>
        <property name="wraplength">325</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="pady">10</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label1">
        <property name="background">#2e2e2e</property>
        <property name="foreground">#ffffff</property>
        <property name="justify">center</property>
        <property name="text" translatable="yes">Seleccionar Subcarpetas?</property>
        <layout manager="pack">
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.OptionMenu" id="opcion_subcarpeta" named="True">
        <property name="value">NO</property>
        <property name="values">SI,NO</property>
        <property name="variable">string:subcarpetas</property>
        <layout manager="pack">
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label2">
        <property name="background">#2e2e2e</property>
        <property name="foreground">#ffffff</property>
        <property name="justify">center</property>
        <property name="text" translatable="yes">Delimitador de caracteres del CSV
(los más comunes son "," ";" "|")</property>
        <layout manager="pack">
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="opcion_delimitador" named="True">
        <layout manager="pack">
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="procesar" named="True">
        <property name="text" translatable="yes">Seleccionar Carpeta y Procesar</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="padx">0</property>
          <property name="pady">4</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label3">
        <property name="background">#2e2e2e</property>
        <property name="foreground">#ffffff</property>
        <property name="justify">center</property>
        <property name="text" translatable="yes">por Agustín Bustos Piasentini
https://www.Agustin-Bustos-Piasentini.com.ar/</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="pady">10</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="cafecito" named="True">
        <property name="text" translatable="yes">Donaciones</property>
        <layout manager="pack">
          <property name="side">top</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
